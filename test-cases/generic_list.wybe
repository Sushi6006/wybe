parameter @ty
pub constructors [] | `[|]`(head:@ty, tail:generic_list(@ty))

pub def length(list:generic_list(@ty)) : int = length1(list, 0)
def length1(list:generic_list(@ty), acc:int) : int =
    if list = [?head | ?tail] then length1(tail, acc+1) else acc

pub def append(a:generic_list(@ty), b:generic_list(@ty)) : generic_list(@ty) =
    if a = [?head | ?tail] then [head|append(tail,b)] else b
