parameter t
pub constructors [] | `[|]`(head:t, tail:generic_list(t))

pub def length(list:generic_list(t)) : int = length1(list, 0)
def length1(list:generic_list(t), acc:int) : int =
    if list = [?head | ?tail] then length1(tail, acc+1) else acc

pub def append(a:generic_list(t), b:generic_list(t)) : generic_list(t) =
    if a = [?head | ?tail] then [head|append(tail,b)] else b
